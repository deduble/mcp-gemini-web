{
  "name": "mcp-gemini-web",
  "version": "0.3.0",
  "description": "MCP server exposing a grounded web search tool powered by Gemini 3 + Google Search grounding, with configurable verbosity, optional sources, and research mode.",
  "type": "module",
  "bin": {
    "mcp-gemini-web": "dist/src/server.js"
  },
  "main": "dist/src/server.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "search",
    "web-search",
    "grounding",
    "research",
    "ai"
  ],
  "author": "Yunus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deduble/mcp-gemini-web.git"
  },
  "homepage": "https://github.com/deduble/mcp-gemini-web#readme",
  "bugs": {
    "url": "https://github.com/deduble/mcp-gemini-web/issues"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/server.ts",
    "start": "node dist/src/server.js",
    "test": "vitest run"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@google/genai": "^0.9.0",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}